{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "useBuiltIns": "entry",
        "targets": {
          "browsers": [
            "edge >= 16",
            "safari >= 9",
            "firefox >= 57",
            "ie >= 9",
            "ios >= 9",
            "chrome >= 49"
          ]
        }
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [["@babel/plugin-transform-runtime", { "corejs": 3 }]]
}
